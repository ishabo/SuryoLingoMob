{
  "name": "suryolingo",
  "version": "0.7.0",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "restart": "yarn stop && yarn clear & yarn start -- --reset-cache",
    "stop": "lsof -nti:8081 | xargs kill -9",
    "test": "jest",
    "ios": "react-native run-ios --simulator=\"4FA1EBFD-BCC3-48BB-B76D-2D9AD8785E82\" && yarn tsc:watch",
    "android": "npx jetify && react-native run-android && yarn tsc:watch",
    "tsc": "node_modules/.bin/tsc",
    "tsc:watch": "yarn  tsc -w",
    "lint": "tslint -c ./tslint.json 'src/**/*.{ts,tsx}' --fix",
    "watch": "yarn build -- -w",
    "remotedev": "remotedev-debugger --injectserver",
    "postinstall": "yarn remotedev-debugger && rndebugger-open",
    "prepush": "yarn test",
    "clear": "yarn cache clean --force && rm -rf $TMPDIR/react-* && rm -rf $TMPDIR/metro-*",
    "bundle:android:prod": "yarn build && react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "release:android:prod": "yarn preStart:prod && yarn bundle:android:prod && cd android && ./gradlew assembleRelease -x bundleReleaseJsAndAssets && cd -",
    "bundle:ios:prod": "yarn build && react-native bundle --dev false --entry-file index.js --bundle-output ios/main.jsbundle --platform ios --assets-dest ios",
    "release:ios:prod": "yarn preStart:prod && yarn bundle:ios:prod",
    "postversion": "react-native-version --never-amend",
    "preStart:prod": "./preStart.sh prod"
  },
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.6.0",
    "@types/jest": "^21.1.5",
    "@types/lodash": "^4.14.77",
    "@types/react": "^16.4.18",
    "@types/react-native": "^0.57.2",
    "@types/react-native-fs": "2.8.0",
    "@types/react-navigation": "^1.2.3",
    "@types/url-parse": "^1.1.0",
    "apisauce": "^0.14.3",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "change-object-case": "^0.1.1",
    "clone-deep": "^2.0.1",
    "concurrently": "^3.5.0",
    "garshonify": "git+https://github.com/ishabo/garshonify#v1.0.2",
    "gatsby-cli": "^2.4.9",
    "glamorous-native": "^1.3.0",
    "globally": "^0.0.0",
    "husky": "^0.14.3",
    "jetifier": "^1.6.4",
    "lodash": "^4.17.4",
    "metro-react-native-babel-preset": "^0.56.0",
    "mockdate": "^2.0.2",
    "native-base": "^2.3.10",
    "react": "16.6.0",
    "react-native": "^0.57.8",
    "react-native-admob": "git+https://github.com/ishabo/react-native-admob",
    "react-native-animatable": "^1.3.0",
    "react-native-audioplayer": "^0.2.0",
    "react-native-blink-view": "^0.0.6",
    "react-native-cached-image": "^1.4.3",
    "react-native-checkbox-field": "^1.1.2",
    "react-native-debugger-open": "^0.3.15",
    "react-native-device-info": "git+https://github.com/psegalen/react-native-device-info.git#fix-dispatch_sync-yellow-box",
    "react-native-dotenv": "0.2.0",
    "react-native-exit-app": "^1.0.0",
    "react-native-fbsdk": "^0.8.0",
    "react-native-firebase": "^5.0.0-rc4",
    "react-native-fs": "2.9.7",
    "react-native-global-font": "^1.0.2",
    "react-native-global-props": "^1.1.3",
    "react-native-i18n": "^2.0.6",
    "react-native-keyboard-aware-scroll-view": "^0.7.2",
    "react-native-keyboard-input": "^5.2.3",
    "react-native-linear-gradient": "2.4.0",
    "react-native-message-bar": "^2.0.7",
    "react-native-modal": "^4.1.1",
    "react-native-popover-tooltip": "git+https://github.com/ishabo/react-native-popover-tooltip#converting_left_to_right",
    "react-native-progress": "^3.4.0",
    "react-native-restart": "^0.0.6",
    "react-native-sensitive-info": "^5.1.0",
    "react-native-snap-carousel": "^3.3.3",
    "react-native-sound": "git+https://github.com/zmxv/react-native-sound",
    "react-native-typescript-transformer": "^1.2.13",
    "react-native-version": "^2.4.1",
    "react-native-version-check": "^3.0.0",
    "react-native-version-number": "^0.3.1",
    "react-navigation": "^1.5.8",
    "react-navigation-redux-helpers": "1.0.3",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-immutable-state-invariant": "^2.1.0",
    "redux-logger": "^3.0.6",
    "redux-persist": "^5.0.0",
    "redux-saga": "^0.16.0",
    "remote-redux-devtools": "^0.5.12",
    "remotedev-rn-debugger": "^0.8.3",
    "rimraf": "^2.6.2",
    "shortid": "^2.2.8",
    "url-parse": "^1.2.0",
    "yarn": "^1.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/plugin-syntax-optional-chaining": "^7.0.0",
    "babel-jest": "23.0.1",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-preset-react-native": "^5.0.2",
    "jest": "21.1.0",
    "react-test-renderer": "16.0.0-alpha.12",
    "schedule": "^0.4.0",
    "ts-jest": "^21.1.3",
    "tslint": "^5.7.0",
    "tslint-config-airbnb": "^5.3.0",
    "typescript": "^3.6"
  },
  "jest": {
    "preset": "react-native",
    "setupFiles": [
      "./jest/setup.js"
    ],
    "globals": {
      "__DEV__": true
    },
    "transform": {
      ".*": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "testMatch": [
      "**/**/*.test.(tsx|ts)"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!react-native-exit-app|react|react-native)"
    ],
    "testEnvironment": "node"
  },
  "rnpm": {
    "assets": [
      "./src/assets/fonts",
      "./src/assets/audio"
    ]
  },
  "workspaces": [
    "src/*"
  ]
}
