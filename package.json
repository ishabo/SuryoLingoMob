{
  "name": "suryolingo",
  "version": "0.7.0",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "restart": "yarn stop && yarn clear & yarn start -- --reset-cache",
    "stop": "lsof -nti:8081 | xargs kill -9",
    "test": "jest",
    "ios": "react-native run-ios && yarn tsc:watch",
    "ios:old": "react-native run-ios --simulator=\"iPhone 8\" && yarn tsc:watch",
    "ios:latest": "react-native run-ios --simulator=\"iPhone 11\" && yarn tsc:watch",
    "android": "npx jetify && react-native run-android && yarn tsc:watch",
    "tsc": "node_modules/.bin/tsc",
    "tsc:watch": "yarn  tsc -w",
    "lint": "tslint -c ./tslint.json 'src/**/*.{ts,tsx}' --fix",
    "prepush": "yarn test",
    "clear": "yarn cache clean --force && rm -rf $TMPDIR/react-* && rm -rf $TMPDIR/metro-*",
    "bundle:android:prod": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "release:android:prod": "yarn preStart:prod && yarn bundle:android:prod && cd android && ./gradlew assembleRelease -x bundleReleaseJsAndAssets && cd -",
    "bundle:ios:prod": "react-native bundle --dev false --entry-file index.js --bundle-output ios/main.jsbundle --platform ios --assets-dest ios",
    "release:ios:prod": "yarn preStart:prod && yarn bundle:ios:prod",
    "postversion": "react-native-version --never-amend",
    "preStart:prod": "./preStart.sh prod",
    "postinstall": "./scripts/rn-patching.sh"
  },
  "dependencies": {
    "@react-native-community/netinfo": "^4.4.0",
    "@react-native-firebase/analytics": "^7.3.1",
    "@react-native-firebase/app": "^8.2.0",
    "@react-native-firebase/crashlytics": "^8.1.2",
    "@react-native-firebase/messaging": "^7.5.0",
    "@types/clone-deep": "^4.0.1",
    "@types/jest": "^21.1.5",
    "@types/lodash": "^4.14.77",
    "@types/react": "^16.8.0",
    "@types/react-native": "^0.60.17",
    "@types/react-native-fbsdk": "^1.1.0",
    "@types/react-native-fs": "2.8.0",
    "@types/react-navigation": "^1.2.3",
    "@types/redux-saga": "^0.10.5",
    "@types/url-parse": "^1.1.0",
    "apisauce": "^0.14.3",
    "change-object-case": "^0.1.1",
    "clone-deep": "^4.0.1",
    "concurrently": "^3.5.0",
    "deprecated-react-native-listview": "^0.0.5",
    "garshonify": "git+https://github.com/ishabo/garshonify#v1.0.2",
    "gatsby-cli": "^2.4.9",
    "glamorous-native": "^2.0.0",
    "globally": "^0.0.0",
    "husky": "^0.14.3",
    "jetifier": "^1.6.4",
    "lodash": "^4.17.4",
    "mockdate": "^3.0.2",
    "native-base": "^2.3.10",
    "react": "16.9.0",
    "react-native": "^0.61.1",
    "react-native-admob": "^2.0.0-beta.6",
    "react-native-animatable": "^1.3.0",
    "react-native-audioplayer": "^0.2.0",
    "react-native-blink-view": "^0.0.6",
    "react-native-cached-image": "git+https://github.com/ishabo/react-native-cached-image",
    "react-native-checkbox-field": "^1.1.2",
    "react-native-debugger-open": "^0.3.15",
    "react-native-device-info": "git+https://github.com/psegalen/react-native-device-info.git#fix-dispatch_sync-yellow-box",
    "react-native-dotenv": "0.2.0",
    "react-native-exit-app": "^1.1.0",
    "react-native-fbsdk": "^2.0.0",
    "react-native-fetch-blob": "^0.10.8",
    "react-native-flash-message": "^0.1.15",
    "react-native-fs": "2.9.7",
    "react-native-global-font": "^1.0.2",
    "react-native-i18n": "^2.0.6",
    "react-native-keyboard-aware-scroll-view": "^0.9.2",
    "react-native-keyboard-input": "^6.0.2",
    "react-native-keyboard-tracking-view": "^5.7.0",
    "react-native-linear-gradient": "2.4.0",
    "react-native-modal": "^4.1.1",
    "react-native-popover-tooltip": "git+https://github.com/ishabo/react-native-popover-tooltip#converting_left_to_right",
    "react-native-progress": "^3.4.0",
    "react-native-restart": "^0.0.13",
    "react-native-sensitive-info": "^5.5.1",
    "react-native-snap-carousel": "^3.3.3",
    "react-native-sound": "^0.11.0",
    "react-native-typescript-transformer": "^1.2.13",
    "react-native-version": "^2.4.1",
    "react-native-version-check": "^3.0.0",
    "react-native-version-number": "^0.3.6",
    "react-native-webview": "^7.4.0",
    "react-navigation": "^1.5.8",
    "react-navigation-redux-helpers": "1.0.3",
    "react-redux": "^5.0.6",
    "react-timer-mixin": "^0.13.4",
    "redux": "^3.7.2",
    "redux-immutable-state-invariant": "^2.1.0",
    "redux-logger": "^3.0.6",
    "redux-persist": "^5.0.0",
    "redux-saga": "^1.1.3",
    "rimraf": "^2.6.2",
    "shortid": "^2.2.8",
    "url-parse": "^1.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/plugin-syntax-optional-chaining": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/runtime": "^7.6.2",
    "babel-jest": "^26.2.2",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "babel-preset-react-native": "^5.0.2",
    "jest": "^26.2.2",
    "metro-react-native-babel-preset": "^0.58.0",
    "react-test-renderer": "16.8.3",
    "remote-redux-devtools": "^0.5.12",
    "remotedev-rn-debugger": "^0.8.4",
    "schedule": "^0.4.0",
    "ts-jest": "^26.1.4",
    "tslint": "^5.7.0",
    "tslint-config-airbnb": "^5.3.0",
    "typescript": "^3.6",
    "yarn": "^1.3.2"
  },
  "jest": {
    "preset": "react-native",
    "setupFiles": [
      "./jest/setup.js"
    ],
    "globals": {
      "__DEV__": true,
      "ts-jest": {
        "tsConfigFile": "tsconfig.jest.json"
      }
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/react-native/jest/preprocessor.js",
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleNameMapper": {
      "@sl(.*)": "<rootDir>/src/$1"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!react-native-exit-app|react|react-native)"
    ],
    "testEnvironment": "node"
  },
  "rnpm": {
    "assets": [
      "./src/assets/fonts",
      "./src/assets/audio"
    ]
  }
}
